program sumArray:

sumAll: takes arr, size: integer array returns integer
    total, i: integer;

    let sum = 0;
    let i = 0;
    while i < size:
        let sum = sum + arr[i]
    end;
    pop sum
end

main:
    arr: integer array;

    let arr = array 5;
    let arr[0] = 10;
    let arr[1] = 22;
    let arr[2] = 18;
    let arr[3] = 46;
    let arr[4] = 14;

    output "The sum of the elements is " . sumAll(arr, 5)
end